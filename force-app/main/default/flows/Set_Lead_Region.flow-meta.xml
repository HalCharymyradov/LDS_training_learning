<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>65.0</apiVersion>
    <assignments>
        <name>Asn_Set_APAC</name>
        <label>Set Region to APAC</label>
        <locationX>314</locationX>
        <locationY>396</locationY>
        <assignmentItems>
            <assignToReference>$Record.Region__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>APAC</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <assignments>
        <name>Asn_Set_EMEA</name>
        <label>Set Region to EMEA</label>
        <locationX>50</locationX>
        <locationY>396</locationY>
        <assignmentItems>
            <assignToReference>$Record.Region__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>EMEA</stringValue>
            </value>
        </assignmentItems>
    </assignments>
    <decisions>
        <name>Dec_Determine_Region</name>
        <label>Determine Region</label>
        <locationX>182</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>No Region Match</defaultConnectorLabel>
        <rules>
            <name>Set_EMEA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>isEMEA</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Asn_Set_EMEA</targetReference>
            </connector>
            <label>Set EMEA</label>
        </rules>
        <rules>
            <name>Set_APAC</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>isAPAC</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Asn_Set_APAC</targetReference>
            </connector>
            <label>Set APAC</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>isAPAC</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!Get_Region_CMT.APAC__c}, {!$Record.Country})</expression>
    </formulas>
    <formulas>
        <name>isEMEA</name>
        <dataType>Boolean</dataType>
        <expression>CONTAINS({!Get_Region_CMT.EMEA__c}, {!$Record.Country})</expression>
    </formulas>
    <interviewLabel>Set Lead Region {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Set Lead Region</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Get_Region_CMT</name>
        <label>Get Region CMT</label>
        <locationX>182</locationX>
        <locationY>158</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Dec_Determine_Region</targetReference>
        </connector>
        <getFirstRecordOnly>true</getFirstRecordOnly>
        <object>Region__mdt</object>
        <storeOutputAutomatically>true</storeOutputAutomatically>
    </recordLookups>
    <start>
        <locationX>56</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>Get_Region_CMT</targetReference>
        </connector>
        <filterLogic>1 AND (2 OR 3)</filterLogic>
        <!-- (1) Country is not an empty string -->
        <filters>
            <field>Country</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <!-- (2) Id is null (on create) -->
        <filters>
            <field>Id</field>
            <operator>IsNull</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <!-- (3) Country is changed (on update) -->
        <filters>
            <field>Country</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Lead</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordBeforeSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
